html
	head
		title Fruits Search Result Pages
		style
			include ../../public/styles/bac.css


	body
		include ../partials/header


		div(id="main")
			div
				h1 Top Search Results for fruits
				br
				each page in pages
					label #{page.title}:
					a(href="/fruits/"+page.title) #{"https://people.scs.carleton.ca/~davidmckenney/fruitgraph/"+page.title}
					br
					label Search Score: #{page.score}
					br
					br



		include ../partials/footer
